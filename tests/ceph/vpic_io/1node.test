#!/bin/bash
source config.nersc
for i in 1 8 32 
#for i in 64 128 256
do
   for j in  1 2 3
   do
	echo "procs:$i,test:$j" >> 1node.test.out.map
 	srun -n $i ./VPIC vpic_p${i}.t${j}.h5 swiftpool $CEPH_CONF 8 >> 1node.test.out.map
	echo "done written vpic_p${i}.t${j}.h5" >> 1node.test.out.map
   done
done
